{
  "schemaVersion": 1,
  "id": "galacticraft-rewoven",
  "version": "$version",

  "name": "Galacticraft: Rewoven",
  "icon": "assets/galacticraft-rewoven/icon.png",
  "description": "FabricMC port of Galacticraft",
  "licence": "Galacticraft License",
  "contact": {
    "sources": "https://github.com/teamgalacticraft/Galacticraft-Rewoven"
  },

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.teamgalacticraft.galacticraft.Galacticraft"
    ],
    "client": [
      "io.github.teamgalacticraft.galacticraft.GalacticraftClient"
    ]
  },
  "mixins": [
    "galacticraft-rewoven.common.json",
    "galacticraft-rewoven.client.json"
  ],
  "requires": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "cotton": "*",
    "cotton-energy": "*",
    "libblockattributes": "*",
    "silk": "*",
    "tgc-utils": "*"
  },
  "custom": {
    "modmenu:clientsideOnly": true,
    "roughlyenoughitems:plugins": [
      {
        "id": "galacticraft_plugin",
        "class": "io.github.teamgalacticraft.galacticraft.recipes.rei.GalacticraftREIPlugin"
      }
    ]
  }
}
